<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta description="Larch Provisions provides fresh-prepared meal service weekly" />
    <meta name="robots" content="index,follow" />
    <meta name="googlebot" content="index,follow" />
    <title>LARCH PROVISIONS</title>

    <environment names="Development">
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <link rel="stylesheet" href="~/css/materialize.css" />
        <link rel="stylesheet" href="~/css/site.css" />
    </environment>
    <environment names="Staging,Production">
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/css/materialize.min.css"
              asp-fallback-href="~/css/materialize.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    </environment>
    @Html.ApplicationInsightsJavaScript(TelemetryConfiguration)
</head>
<body>
    <header>
        <div class="navbar-fixed">
            <nav>
                <div class="nav-wrapper">
                    <a href="/" class="brand-logo waves-effect waves-ripple left">
                        <img src="~/img/lil-larch.png" />
                    </a>
                    <a data-activates="collapse-menu" class="button-collapse right"><i class="material-icons">menu</i></a>
                    <ul id="nav-mobile" class="right hide-on-med-and-down">
                        <li>@Html.ActionLink("provisions", "Provisions", "Home")</li>
                        <li><a href="https://docs.google.com/forms/d/1BhouNqEv1ejjYTUU41yzYbvudVlrwFAT5kq7BelMW1s/viewform" target="_blank">meals</a></li>
                        <li>@Html.ActionLink("catering", "Contact", "Home")</li>
                    </ul>
                    <ul class="side-nav right-align" id="collapse-menu">
                        <li>@await Html.PartialAsync("_LoginPartial")</li>
                        <li>@Html.ActionLink("provisions", "Provisions", "Home")</li>
                        <li><a href="https://docs.google.com/forms/d/1BhouNqEv1ejjYTUU41yzYbvudVlrwFAT5kq7BelMW1s/viewform" target="_blank">meals</a></li>
                        <li>@Html.ActionLink("catering", "Contact", "Home")</li>
                        <li>@Html.ActionLink("about", "About", "Home")</li>
                        @if (User.IsInRole("Admin"))
                        {
                            <li>@Html.ActionLink("admin", "Index", "Admin")</li>
                        }
                        @if (User.IsInRole("Chef"))
                        {
                            <li>@Html.ActionLink("menu list", "Index", "Menus")</li>
                            <li>@Html.ActionLink("recipe list", "Index", "Recipes")</li>
                            <li>@Html.ActionLink("ingredient list", "Index", "Ingredients")</li>

                        }
                    </ul>
                </div>
            </nav>
        </div>
    </header>
    <main class="container-fluid body-content">
        @RenderBody()
    </main>
    <footer class="page-footer">
        <div class="row">
            <div class="col s6 m6 l6 feet">
                <a href="/">larch provisions</a>
                <br />
                <a href="https://docs.google.com/forms/d/1BhouNqEv1ejjYTUU41yzYbvudVlrwFAT5kq7BelMW1s/viewform">place an order</a>
            </div>
            <div class="col s6 m6 l6 right-align">
                <a href="/">&copy; 2016 larch provisions</a>
                <br />
                <a href="http://larchprovisions.com">website made by doctorjuno</a>
            </div>
        </div>
    </footer>

    <environment names="Development">
        <script src="~/lib/jquery/dist/jquery.js" asp-append-version="false"></script>
        <script src="~/lib/Materialize/dist/js/materialize.js" asp-append-version="false"></script>
        <script>
            (function (i, s, o, g, r, a, m) {
                i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                    (i[r].q = i[r].q || []).push(arguments)
                }, i[r].l = 1 * new Date(); a = s.createElement(o),
                m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
            })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

            ga('create', 'UA-78821041-1', 'auto');
            ga('send', 'pageview');
        </script>
        <script src="~/js/site.es5.js" asp-append-version="false"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.1.4.min.js"
                asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
                asp-fallback-test="window.jQuery">
        </script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/js/materialize.min.js"
                asp-fallback-src="~/lib/Materialize/dist/js/materialize.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal">
        </script>
        <script>
            (function (i, s, o, g, r, a, m) {
                i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                    (i[r].q = i[r].q || []).push(arguments)
                }, i[r].l = 1 * new Date(); a = s.createElement(o),
                m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
            })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

            ga('create', 'UA-78821041-1', 'auto');
            ga('send', 'pageview');
        </script>
        <script src="~/js/site.es5.min.js" asp-append-version="true"></script>
    </environment>

    @RenderSection("scripts", required: false)
</body>
</html>